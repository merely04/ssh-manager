{
  "name": "ssh-manager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev:eel": "venv/Scripts/python main.py --develop",
    "dev:react": "vite --port 4000",
    "dev": "concurrently \"yarn dev:react\" \"yarn dev:eel\"",
    "build:react": "tsc && vite build",
    "build:eel": "venv/Scripts/python -m eel main.py build --onefile --noconsole --name ssh-manager",
    "preview": "vite preview",
    "start": "venv/Scripts/python main.py --production"
  },
  "dependencies": {
    "@effector/reflect": "^8.3.1",
    "compose-function": "^3.0.3",
    "effector": "^22.8.0",
    "effector-react": "^22.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@types/compose-function": "^0.0.30",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.0.1",
    "effector-inspector": "^0.14.0",
    "effector-logger": "^0.13.7",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.1.0"
  }
}
