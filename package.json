{
  "name": "ssh-manager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "pretest": "yarn lint:fix",
    "format": "prettier --config ./.prettierrc -w src/**/*.{tsx,ts} && git update-index --again",
    "dev:eel": "venv/Scripts/python main.py --develop",
    "dev:react": "vite --port 4000",
    "dev": "concurrently \"yarn dev:react\" \"yarn dev:eel\"",
    "build:react": "tsc && vite build",
    "build:eel": "venv/Scripts/python -m eel main.py build --onefile --noconsole --name ssh-manager",
    "preview": "vite preview",
    "start": "venv/Scripts/python main.py --production",
    "prepare": "husky install"
  },
  "dependencies": {
    "@effector/reflect": "^8.3.1",
    "compose-function": "^3.0.3",
    "effector": "^22.8.0",
    "effector-react": "^22.5.1",
    "patronum": "^1.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/compose-function": "^0.0.30",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^8.0.1",
    "effector-inspector": "^0.14.0",
    "effector-logger": "^0.13.7",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.1.0"
  }
}
